<html>
    <head>
        <title><%=title %></title>
    </head>
    <body>
        <h1>Contact List</h1>
        <div>
            <ul>
                <% for(let i of contact_list) {%>
                    <li>
                        <p> <%=i.name%></p>
                        <p><%= i.phone%></p>
                    </li>
                <%}%>
            </ul>
        </div>

        <form action="/contact-list" method="POST">
            <input type="text" name="name" placeholder="Enter Name">
            <input type="text" name="phone" placeholder="Enter Phone">
            <button type="submit">Add Contact</button>
        </form>

    </body>
</html>